{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
    },
    "extends": "airbnb",
    "rules": {
        "indent": [ "error", 4 ],
        "react/jsx-indent": [ "error", 4 ],
        "react/jsx-indent-props": [ "error", 4 ],
        "jsx-a11y/anchor-is-valid": [ "error", {
            "components": [ "a" ],
        }]
    }
}
